---
import BlogLayout from "@/layouts/BlogLayout.astro";
import Navigation from "@/components/Navigation.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BlogLayout title={title}>
  <!-- <Title title={title} />	 -->
  <Navigation
    links={[
      {
        name: "Home",
        href: "/",
      },
      {
        name: "About",
        href: "/about",
      },
      {
        name: "Blog",
        href: "/blog",
      },
      {
        name: "Contact",
        href: "/contact",
      },
    ]}
  />
</BlogLayout>
